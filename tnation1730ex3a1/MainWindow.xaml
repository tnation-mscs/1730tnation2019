            <TextBox x:Name="resultTextBox5" Grid.Column="4" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Center" Width="170" Margin="10,3,0,2" Grid.Row="5"/>
            <Label Content="6) Sum, while" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="6"/>
            <TextBox x:Name="inputTextBox6a" Grid.Column="1" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Center" Width="164" Grid.Row="6" Text="22 14 5 47 12 " Grid.ColumnSpan="2" Margin="0,3"/>
            <TextBox x:Name="inputTextBox6b" Grid.Column="3" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Center" Width="60" Grid.Row="6" Text="5"/>
            <TextBox x:Name="resultTextBox6" Grid.Column="4" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Center" Width="170" Margin="10,2,0,3" Grid.Row="6"/>
            <Label Content="7) Sum, do-while" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="7"/>
            <TextBox x:Name="inputTextBox7a" Grid.Column="1" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Center" Width="164" Grid.Row="7" Text="22 14 5 47 12 " Grid.ColumnSpan="2" Margin="0,3"/>
            <TextBox x:Name="inputTextBox7b" Grid.Column="3" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Center" Width="60" Grid.Row="7" Text="5"/>
            <TextBox x:Name="resultTextBox7" Grid.Column="4" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Center" Width="170" Margin="10,2,0,3" Grid.Row="7"/>
            <Label Content="8) Sum, for" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="8"/>
            <TextBox x:Name="inputTextBox8a" Grid.Column="1" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Center" Width="164" Grid.Row="8" Text="22 14 5 47 12 " Grid.ColumnSpan="2" Margin="0,3"/>
            <TextBox x:Name="inputTextBox8b" Grid.Column="3" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Center" Width="60" Grid.Row="8" Text="5"/>
            <TextBox x:Name="resultTextBox8" Grid.Column="4" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Center" Width="170" Margin="10,2,0,3" Grid.Row="8"/>



            <Button x:Name="calcButton" Content="Calculate" Grid.Column="1" HorizontalAlignment="Left" Height="22" Grid.Row="9" VerticalAlignment="Center" Width="60" RenderTransformOrigin="3.217,-1.909" Click="CalcButton_Click"/>
        </Grid>

    </Grid>
</Window>
